generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Registration {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  rollNo    String
  name      String
  email     String
  year      Int
  gender    String
  dob       DateTime
  address   String
  cv        String // URL for the uploaded CV
  linkedin  String
  github    String
  bestFit   String
  skillsContribution String
}

